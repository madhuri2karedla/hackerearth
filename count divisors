// Write your code here
#include<iostream>
 using namespace std;
 int main(){
	 int l,r,k,c=0;
	 cin>>l>>r>>k;
	 for(int i=l;i<=r;i++)
	 {
		 if(i%k==0)
		 c++;
	 }
	 cout<<c;
 }
