// Write your code here
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int nz=0,no=0;  //nz = no. of Zs , no = no. of Os
    string s;
    cin>>s;
    for(int i=0;i<s.length();i++)
    {
        if(s[i]=='z')
        {
           nz++;
        }
        else
        {
            no++;
        }
    }
    if(2*nz==no)
    cout<<"Yes"<<endl;
    else
    cout<<"No"<<endl;
}
